# Puppet Name: <%= @title %>
#
<%
capath = scope['::splunk::capath']
enablesplunkwebssl = scope['::splunk::params::webssl']
privkeypath = scope['::splunk::params::privkeypath']
webcertpath = scope['::splunk::params::webcertpath']
ciphersuite = scope['::splunk::params::ciphersuite']
sslversions = scope['::splunk::params::sslversions']
sslnegotiation = scope['::splunk::params::sslnegotiation']
-%>
[settings]
enableSplunkWebSSL = <%= enablesplunkwebssl %>
allowSslRenegotiation = <%= sslnegotiation %>
privKeyPath = <%= capath -%>/splunkweb/<%= privkeypath %>
serverCert = <%= capath -%>/splunkweb/<%= webcertpath %>
cipherSuite = <%= ciphersuite %>
sslVersions = <%= sslversions %>
