# Puppet Name: <%= title %>
#
[<%= title -%>]
<%
if title == 'main'
  name = 'defaultdb'
else
  name = title
end
-%>
homePath = volume:splunk_warm/<%= name -%>/db
coldPath = volume:splunk_cold/<%= name -%>/colddb
thawedPath = $SPLUNK_DB/<%= name -%>/thaweddb
frozenTimePeriodInSecs = <%= frozen_tTime -%>
