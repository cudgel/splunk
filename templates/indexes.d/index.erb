<% if @title == 'main'
  name = 'defaultdb'
else
  name = @title
end -%>
# Puppet Name: <%= @title %>
#
[<%= @title -%>]

homePath = volume:splunk_warm/<%= name -%>/db
coldPath = volume:splunk_cold/<%= name -%>/colddb
thawedPath = $SPLUNK_DB/<%= name -%>/thaweddb
frozenTimePeriodInSecs = <%= frozen_time -%>
<% if @options != nil -%><% @options.each do |op| -%>
<%= op %>
<% end %>i<% end -%>
