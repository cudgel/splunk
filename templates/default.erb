<%
    host = "#{hostname}.es.gwu.edu"
-%>
[default]
host = <%= host %>

[monitor:/<%= splunkhome %>/var/log/splunk]
index = _internal
_TCP_ROUTING = *
_whitelist = /(audit|license_audit|metrics|splunkd|splunkd_stderr|splunkd_stdout|splunklogger)\.log$

[filter:blacklist:splunkvar]
regex1 = ^<%= splunkhome %>/var

[fschange:<%= splunkhome %>]
index = _audit
disabled = false
pollPeriod = 60
recurse = true
followLinks = false
hashMaxSize = -1
fullEvent = false
sendEventMaxSize = -1
filesPerDelay = 10
delayInMills = 100
signedaudit = false
filters = splunkvar
